generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model resident {
  residentId Int     @id @default(autoincrement())
  username   String  @unique
  password   String
  email      String  @unique
  name       String
  phone      String
  status     Boolean @default(false)
  role       String? @default("resident")
}
